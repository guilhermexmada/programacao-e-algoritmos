algoritmo "word2ex1"

var
media, p1, p2, p3 : real

inicio
escreva ("Digite a nota da primeira prova: ")
leia (p1)
escreva ("Digite a nota da segunda prova: ")
leia (p2)
escreva ("Digite a nota da terceira prova: ")
leia (p3)
media <- (p1*2)+(p2*3)+(p3*5)/10
escreva ("A sua média final é",media)
fimalgoritmo
