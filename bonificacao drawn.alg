algoritmo "semnome"

var
quantvendas: inteiro
bonificacao, salariototal: real
inicio
escreva ("Digite a quantidade de softwares vendidos: ")
leia (quantvendas)
bonificacao <- quantvendas*80
salariototal <- bonificacao + 1200
escreva ("Seu salário final, bonificado, é de R$ ",salariototal,",00")

fimalgoritmo
