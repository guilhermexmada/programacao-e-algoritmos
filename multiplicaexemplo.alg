algoritmo "multiplica_numero"

var
mult, n1, n2, n3: inteiro

inicio
Escreva ("Digite o primeiro número: ")
leia (n1)
escreva ("Digite o segundo número: ")
leia (n2)
escreva ("Digite o terceiro número: ")
leia (n3)
mult <- n1*n2*n3
escreva ("O resultado da multiplicação dos números é: ", mult)
fimalgoritmo
