algoritmo "ex5word"

var
num1, num2, num3, soma, quadsoma : real

inicio
escreva ("Digite o primeiro número : ")
leia (num1)
escreva ("Digite o segundo número : ")
leia (num2)
escreva ("Digite o terceiro número : ")
leia (num3)
soma <- num1+num2+num3
quadsoma <- soma ^ 2
escreval ("A soma dos números resulta em : ",soma)
escreval ("O quadrado da soma resulta em : ",quadsoma)

fimalgoritmo
